{
  "name": "flight-common",
  "version": "0.0.0",
  "description": "Shared utilities for Alces Flight web apps",
  "files": [
    "src",
    "dist",
    "lib"
  ],
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib",
    "clean": "rm -rf ./dist ./lib",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src:$PWD/src/modules NODE_ENV=test mocha --compilers js:babel-register --recursive 'src/**/__tests__/*'  --require src/__tests__/setup.js",
    "test:watch": "npm run test -- --watch",
    "uninstall-dependencies": "for package in $( ls node_modules ) ; do npm uninstall $package ; done"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "author": "Alces Flight Limited",
  "license": "All Rights Reserved",
  "private": true,
  "peerDependencies": {
    "react": ">=15.0",
    "react-dom": ">=15.0",
    "react-router-dom": ">=4.0.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "clipboard": "^1.5.15",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.2",
    "moment": "^2.16.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-cookie-banner": "0.0.17",
    "react-fontawesome": "^1.4.0",
    "react-redux": "^4.4.6",
    "react-timeago": "^3.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.19.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "delicate-error-reporter": "0.0.9",
    "eslint": "3.10.2",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "6.7.1",
    "estraverse-fb": "1.3.1",
    "html-loader": "^0.4.5",
    "markdown-loader": "^2.0.0",
    "node-sass": "3.13.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4"
  }
}
