{
  "name": "flight-common",
  "version": "0.0.0",
  "description": "Shared utilities for Alces Flight web apps",
  "engines": {
    "node": "4.1.2"
  },
  "files": [
    "src",
    "dist",
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack --progress --colors --watch",
    "build:lib": "babel src --out-dir lib",
    "build:umd:dev": "NODE_ENV=development webpack --colors --progress src/index.js dist/flight-common.dev.js",
    "build:umd:prod": "NODE_ENV=production webpack --colors --progress src/index.js dist/flight-common.min.js",
    "build": "npm run build:lib && npm run build:umd:dev && npm run build:umd:prod",
    "clean": "rm -rf ./dist ./lib",
    "compress-bundles": "scripts/compress-bundles.sh everything",
    "dev-server": "NODE_ENV=development node server.js",
    "docker-bootstrap": "npm run docker-build && npm run docker-run npm run uninstall-dependencies && npm run docker-run npm install && npm run docker-dev-server",
    "docker-build": "docker build -t aviator --build-arg USER_ID=\"$(id -u)\" .",
    "docker-dev-server": "npm run docker-run npm run dev-server",
    "docker-run": "scripts/docker-run",
    "docker-test": "npm run docker-run 4001 npm test",
    "docker-test:watch": "npm run docker-run 4001 npm run test:watch",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "preinstall": "npm run clean && npm run build",
    "shrinkpack": "shrinkpack",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src:$PWD/src/modules NODE_ENV=test mocha --compilers js:babel-register --recursive 'src/**/__tests__/*'  --require src/__tests__/setup.js",
    "test:watch": "npm run test -- --watch",
    "uninstall-dependencies": "for package in $( ls node_modules ) ; do npm uninstall $package ; done",
    "upload-assets": "scripts/upload-to-cloud-storage.sh"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "author": "Alces Flight Limited",
  "license": "All Rights Reserved",
  "private": true,
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "react-router": ">=2.0.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.2.0",
    "classnames": "^2.2.0",
    "clipboard": "^1.5.5",
    "debounce": "^1.0.0",
    "extended-proptypes": "^1.1.3",
    "faye": "^1.1.2",
    "font-awesome": "^4.5.0",
    "history": "^2.0.1",
    "js-yaml": "^3.4.6",
    "lodash": "^4.15.0",
    "moment": "^2.10.6",
    "react-addons-css-transition-group": "^0.14.2",
    "react-addons-update": "^0.14.5",
    "react-bootstrap": "^0.28.1",
    "react-data-components": "^0.6.0",
    "react-fontawesome": "^0.3.3",
    "react-prop-types": "^0.3.0",
    "react-redux": "^4.0.0",
    "react-router-bootstrap": "^0.20.1",
    "react-select": "^0.6.12",
    "react-timeago": "^2.2.1",
    "redux": "^3.0.0",
    "redux-logger": "^2.0.0",
    "redux-simple-promise": "git://github.com/benarmston/redux-simple-promise.git#do-not-clobber-action-meta-build",
    "reselect": "^2.0.0",
    "zxcvbn": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-react-constant-elements": "^6.4.0",
    "babel-plugin-transform-react-inline-elements": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.4.2",
    "css-loader": "^0.23.0",
    "delicate-error-reporter": "0.0.9",
    "enzyme": "^2.1.0",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^4.2.3",
    "estraverse-fb": "^1.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "jsx-chai": "^2.0.0",
    "markdown-loader": "^0.1.7",
    "mocha": "^2.3.4",
    "node-sass": "^3.3.3",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.8",
    "react-router": "^2.7.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "sass-loader": "^3.1.1",
    "shrinkpack": "^0.13.1",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-sources": "^0.1.0"
  }
}
